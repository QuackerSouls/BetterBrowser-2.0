{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PyBrowser Debug",
            "type": "python",
            "request": "launch",
            "program": "advanced_browser.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "PyBrowser Release",
            "type": "python",
            "request": "launch",
            "program": "advanced_browser.py",
            "console": "none",
            "cwd": "${workspaceFolder}"
        }
    ]
}